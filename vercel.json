{
  "version": 2,
  "builds": [
    {
      "src": ".vitepress/dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ".vitepress/dist/$1"
    }
  ],
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=3600"
        }
      ]
    }
  ]
}